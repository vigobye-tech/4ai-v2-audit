{
  "name": "4ai-lab-v2",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint src --ext ts",
    "format": "prettier --write src",
    "test": "vitest",
    "check:all": "tsc --noEmit && cd src-tauri && cargo check",
    "audit:lightning": "node scripts/audit.js lightning && npm run type-check",
    "audit:security": "node scripts/audit.js security",
    "audit:selectors": "node scripts/audit.js selectors",
    "audit:resources": "node scripts/audit.js resources",
    "audit:performance": "node scripts/audit.js performance",
    "audit:full": "node scripts/audit.js full",
    "monitor:daily": "node scripts/monitor-webai.js --format=text",
    "monitor:weekly": "node scripts/monitor-webai.js --format=json",
    "monitor:health": "node scripts/monitor-webai.js --live --format=text",
    "monitor:claude": "node scripts/monitor-webai.js --service=claude --format=text",
    "monitor:chatgpt": "node scripts/monitor-webai.js --service=chatgpt --format=text",
    "monitor:gemini": "node scripts/monitor-webai.js --service=gemini --format=text",
    "monitor:copilot": "node scripts/monitor-webai.js --service=copilot --format=text",
    "manus:emergency": "node tools/manus-orchestrator.js --task=emergency-fix --auto",
    "manus:maintenance": "node tools/manus-orchestrator.js --task=scheduled-maintenance",
    "manus:diagnostic": "node tools/manus-orchestrator.js --task=diagnostic-deep-dive",
    "diagnostic:demo": "node diagnostics/diagnostic-demo.js",
    "diagnostic:quick": "node scripts/monitor-webai.js",
    "diagnostic:full": "node diagnostics/tools/webai-diagnostic.js --auto-update",
    "diagnostic:chatgpt": "node diagnostics/tools/webai-diagnostic.js --service=chatgpt --headless=false",
    "diagnostic:claude": "node diagnostics/tools/webai-diagnostic.js --service=claude --headless=false",
    "diagnostic:gemini": "node diagnostics/tools/webai-diagnostic.js --service=gemini --headless=false",
    "diagnostic:api": "node diagnostics/tools/diagnostic-api.js",
    "manus:performance": "node tools/manus-orchestrator.js --task=performance-optimization --auto",
    "tools:dom-inspect": "node tools/dom-inspector.js",
    "tools:error-analyze": "node tools/error-analyzer.js",
    "tools:config-update": "node tools/config-updater.js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.4.0",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.0",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@tauri-apps/plugin-shell": "^2.3.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.4.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "glob": "^11.0.3",
    "prettier": "^3.0.0",
    "puppeteer": "^22.15.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  }
}
